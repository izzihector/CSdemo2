<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="project_formulier_image" inherit_id="project_formulier.project_formulier_image">
        <xpath expr="." position="inside">
            <t t-if="formulier_id.xaa_aa_formulier_type == 'formulier_two'">
                <div class="form-group row form-field" id="formulier_2_form">
                    <input type="file" name="xaa_aa_house" class="hide_input_image form-control o_website_form_input"/>
                    <input type="file" name="xaa_aa_access" class="hide_input_image form-control o_website_form_input"/>
                    <input type="file" name="xaa_aa_parking_spot" class="hide_input_image form-control o_website_form_input"/>
                    <input type="file" name="xaa_aa_obstacles" class="hide_input_image form-control o_website_form_input"/>
                    <input type="file" name="xaa_aa_wall1" class="hide_input_image form-control o_website_form_input"/>
                    <input type="file" name="xaa_aa_wall2" class="hide_input_image form-control o_website_form_input"/>
                    <input type="file" name="xaa_aa_floor1" class="hide_input_image form-control o_website_form_input"/>
                    <input type="file" name="xaa_aa_roof1" class="hide_input_image form-control o_website_form_input"/>
                    <div class="col-lg-3 col-md-3 col-sm-3 custom-text">
                        <h5>House / Property</h5>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-3">
                        <button aria-label="Edit" class="fa fa-pencil pull-right fa-lg edit_button" title="Edit" ></button>
                        <img class="img img-fluid" id="xaa_aa_house" t-attf-src="/web/image/question.formulier/{{formulier_id.id}}/xaa_aa_house"/>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-3 custom-text">
                        <h5>Access</h5>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-3">
                        <button aria-label="Edit" class="fa fa-pencil pull-right fa-lg edit_button" title="Edit" ></button>
                        <img class="img img-fluid" id='xaa_aa_access' t-attf-src="/web/image/question.formulier/{{formulier_id.id}}/xaa_aa_access"/>
                    </div>
                </div>
                <div class="form-group row form-field">
                    <div class="col-lg-3 col-md-3 col-sm-3 custom-text">
                        <h5>Parking spot</h5>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-3">
                        <button aria-label="Edit" class="fa fa-pencil pull-right fa-lg edit_button" title="Edit" ></button>
                        <img class="img img-fluid" id="xaa_aa_parking_spot" t-attf-src="/web/image/question.formulier/{{formulier_id.id}}/xaa_aa_parking_spot"/>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-3 custom-text">
                        <h5>Obstacles</h5>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-3">
                        <button aria-label="Edit" class="fa fa-pencil pull-right fa-lg edit_button" title="Edit" ></button>
                        <img class="img img-fluid" id="xaa_aa_obstacles" t-attf-src="/web/image/question.formulier/{{formulier_id.id}}/xaa_aa_obstacles"/>
                    </div>
                </div>
                <div class="form-group row form-field">
                    <div class="col-lg-3 col-md-3 col-sm-3 custom-text">
                        <h5>Wall 1</h5>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-3">
                        <button aria-label="Edit" class="fa fa-pencil pull-right fa-lg edit_button" title="Edit" ></button>
                        <img class="img img-fluid" id="xaa_aa_wall1" t-attf-src="/web/image/question.formulier/{{formulier_id.id}}/xaa_aa_wall1"/>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-3 custom-text">
                        <h5>Wall 2</h5>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-3">
