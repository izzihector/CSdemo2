<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="online_formulier_pv_quote">
        <t t-if="formulier_id.xaa_aa_formulier_type == 'formulier_three'">
            <div class="form-group row form-field">
                <div class="col-lg-3 col-md-4 col-sm-4 col-lg-offset-1 col-md-offset-1 custom-text">
                    <label class="col-form-label" for="xaa_aa_energy_use">How much energy You use ?</label>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6 col-9">
                    <input type="number" name="xaa_aa_energy_use" class="form-control" t-att-value="formulier_id.xaa_aa_energy_use" min="1"/>
                </div>
                <div class="">
                    <button class="btn btn-primary reset_value" type="reset" >Reset</button>
                </div>
            </div>
            <div class="form-group row form-field">
                <div class="col-lg-3 col-md-4  col-sm-4 col-lg-offset-1 col-md-offset-1 custom-text">
                    <label class="col-form-label" for="xaa_aa_location_correction">Location Correction</label>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <input type="number" name="xaa_aa_location_correction" class="form-control" t-att-value="formulier_id.xaa_aa_location_correction" placeholder="Enter a whole number, not a decimal" min="1"/>
                </div>
            </div>
            <div class="form-group row form-field">
                <div class="col-lg-3 col-md-4  col-sm-4 col-lg-offset-1 col-md-offset-1 custom-text">
                    <label class="col-form-label" for="xaa_aa_location_calculation">Location Calculations</label>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <input type="number" name="xaa_aa_location_calculation" class="form-control" t-att-value="formulier_id.xaa_aa_location_calculation" readonly="" min="1"/>
                </div>
            </div>
            <div class="form-group row form-field">
                <div class="col-lg-3 col-md-4 col-sm-4 col-lg-offset-1 col-md-offset-1 custom-text">
                    <label class="col-form-label" for="xaa_aa_solar_type">Solar type</label>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <div class="div_product">
                        <select class="form-control" name="xaa_aa_solar_type" multiple="multiple">
                            <t t-foreach="solar_type_ids" t-as="st">
                                <option t-att-value="st.id" t-att-selected="st.id in formulier_id.xaa_aa_solar_type.ids">
                                    <t t-esc="st.xaa_aa_name"/>
                                </option>
                            </t>
                        </select>
                    </div>
                </div>
            </div>
            <div class="solar_product">
                <div class="div_padding_left" >
                    <div class="col-lg-3 col-md-4 col-sm-4 col-lg-offset-1 col-md-offset-1 custom-text">
                        <label class="col-form-label" for="xaa_aa_solar_product">Which solar panel do you want?</label>
                    </div>
                    <div class="row col-lg-12 col-md-12 col-sm-12">
                        <div class="solar_product_section"
                            style="display:block; list-style-type:none;">
                            <t t-foreach="solar_product_ids" t-as="solar_prod">
                                <div t-att-class="'col-lg-6 col-md-6 col-sm-6 col-12 padding_14_10 solar_product_li product_li selected text-center mt8' if formulier_id.xaa_aa_solar_product and formulier_id.xaa_aa_solar_product.id == solar_prod.id else 'col-lg-6 col-md-6 col-sm-6 col-12 padding_14_10 solar_product_li product_li text-center mt8'" t-att-data-id="solar_prod.id">
                                    <div class="text-center mt4">
                                        <span t-if="solar_prod.xaa_aa_wq_header_1" style="font-size:16px;" t-esc="solar_prod.xaa_aa_wq_header_1"/>
                                        <span t-if="not solar_prod.xaa_aa_wq_header_1" style="font-size:16px;" t-esc="solar_prod.name"/><br/>
                                        <span t-if="solar_prod.xaa_aa_wq_header_2" style="font-size:14px;" t-esc="solar_prod.xaa_aa_wq_header_2"/>
                                    </div>
                                    <div class="img_house_info">
                                        <img class="img img-fluid" t-attf-src="/web/image/product.product/{{solar_prod.id}}/xaa_aa_wq_image"/>
                                    </div>
                                    <div class="text-left">
                                        <t t-if="solar_prod.xaa_aa_wq_line1">
                                            <b><span style="font-size:12px;" t-esc="solar_prod.xaa_aa_wq_line1"/></b><br/>
                                        </t>
                                        <t t-if="solar_prod.xaa_aa_wq_line2">
                                            <b><span style="font-size:12px;" t-esc="solar_prod.xaa_aa_wq_line2"/></b><br/>
                                        </t>
                                        <t t-if="solar_prod.xaa_aa_wq_line3">
                                            <b><span style="font-size:12px;" t-esc="solar_prod.xaa_aa_wq_line3"/></b><br/>
                                        </t>
                                        <t t-if="solar_prod.xaa_aa_wq_line4">
                                            <b><span style="font-size:12px;" t-esc="solar_prod.xaa_aa_wq_line4"/></b>
                                        </t>
                                    </div>
                                </div>
                            </t>
                        </div>
                        <div class="solar_product_qty_show" t-att-style="'display:block;'if formulier_id.xaa_aa_solar_product else 'display:none;'">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="snipper_Add_Minus mt24">
                                    <button type="button" class="pf_sub remove_solar_qty update_solar_qty" t-att-value="formulier_id.xaa_aa_solar_product.id">-</button>
                                    <input type="text" name="" class="solar_qty update_solar_qty" value="1"/>
                                    <button type="button" class="pf_add add_solar_qty update_solar_qty" t-att-value="formulier_id.xaa_aa_solar_product.id">+</button>
                                    <span class="montserrat_regular ml8 mb8">QTY</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-group row form-field mt16">
                <div class="col-lg-3 col-md-4 col-sm-4 col-lg-offset-1 col-md-offset-1 custom-text">
                    <label class="col-form-label" for="xaa_aa_solar_watt_piek">Solar Energy Watt Piek</label>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <input type="number" name="xaa_aa_solar_watt_piek" class="form-control" t-att-value="formulier_id.xaa_aa_solar_watt_piek"/>
                </div>
            </div>
            <div class="form-group row form-field">
                <div class="col-lg-3 col-md-4 col-sm-4 col-lg-offset-1 col-md-offset-1 custom-text">
                    <label class="col-form-label" for="xaa_aa_energy_wat_piek">Energy Wat Piek Calclulation</label>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <input type="number" name="xaa_aa_energy_wat_piek" class="form-control" t-att-value="formulier_id.xaa_aa_energy_wat_piek" readonly=""/>
                </div>
            </div>
            <div class="form-group row form-field">
                <div class="col-lg-3 col-md-4 col-sm-4 col-lg-offset-1 col-md-offset-1 custom-text">
                    <label class="col-form-label" for="xaa_aa_need_converter">Do You need Converter ?</label>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <div>
                        <select name="xaa_aa_need_converter" class="form-control">
                            <option></option>
                            <option value='ja' t-att-selected="formulier_id.xaa_aa_need_converter == 'ja'">
                                    ja
                                </option>
                                <option value='nee' t-att-selected="formulier_id.xaa_aa_need_converter == 'nee'">
                                    nee
                                </option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="form-group row form-field converter_field" t-att-style="'display:block;' if formulier_id.xaa_aa_need_converter == 'ja' else 'display:none;'">
                <div class="div_padding_left" >
                    <div class="col-lg-3 col-md-4 col-sm-4 col-lg-offset-1 col-md-offset-1 custom-text">
                        <label class="col-form-label" for="formulier_id.xaa_aa_converter_product">Which converter do you want ?</label>
                    </div>
                    <div class="row col-lg-12 col-md-12 col-sm-12">
                        <div class="converter_product_section"
                            style="display:block; list-style-type:none;">
                            <t t-foreach="converter_product_ids" t-as="cp">
                                <div t-att-class="'col-lg-6 col-md-6 col-sm-6 col-12 padding_14_10 converter_product_li product_li selected text-center mt8' if formulier_id.xaa_aa_converter_product and formulier_id.xaa_aa_converter_product.id == cp.id else 'col-lg-6 col-md-6 col-sm-6 col-12 padding_14_10 converter_product_li product_li text-center mt8'" t-att-data-id="cp.id">
                                    <div class="text-center mt4">
                                        <span t-if="cp.xaa_aa_wq_header_1" style="font-size:16px;" t-esc="cp.xaa_aa_wq_header_1"/>
                                        <span t-if="not cp.xaa_aa_wq_header_1" style="font-size:16px;" t-esc="cp.name"/><br/>
                                        <span t-if="cp.xaa_aa_wq_header_2" style="font-size:14px;" t-esc="cp.xaa_aa_wq_header_2"/>
                                    </div>
                                    <div class="img_house_info">
                                        <img class="img img-fluid" t-attf-src="/web/image/product.product/{{cp.id}}/xaa_aa_wq_image"/>
                                    </div>
                                    <div class="text-left">
                                        <t t-if="cp.xaa_aa_wq_line1">
                                            <b><span style="font-size:12px;" t-esc="cp.xaa_aa_wq_line1"/></b><br/>
                                        </t>
                                        <t t-if="cp.xaa_aa_wq_line2">
                                            <b><span style="font-size:12px;" t-esc="cp.xaa_aa_wq_line2"/></b><br/>
                                        </t>
                                        <t t-if="cp.xaa_aa_wq_line3">
                                            <b><span style="font-size:12px;" t-esc="cp.xaa_aa_wq_line3"/></b><br/>
                                        </t>
                                        <t t-if="cp.xaa_aa_wq_line4">
                                            <b><span style="font-size:12px;" t-esc="cp.xaa_aa_wq_line4"/></b>
                                        </t>
                                    </div>
                                </div>
                            </t>
                        </div>
                        <div class="converter_product_qty_show" t-att-style="'display:block;' if formulier_id.xaa_aa_converter_product else 'display:none;'">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="snipper_Add_Minus mt24">
                                    <button type="button" class="pf_sub remove_converter_qty update_converter_qty" t-att-value="formulier_id.xaa_aa_converter_product.id">-</button>
                                    <span class="product_qty converter_qty">1</span>
                                    <button type="button" class="pf_add add_converter_qty update_converter_qty" t-att-value="formulier_id.xaa_aa_converter_product.id">+</button>
                                    <span class="montserrat_regular ml8 mb8">QTY</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-group row form-field">
                <div class="col-lg-3 col-md-4 col-sm-4 col-lg-offset-1 col-md-offset-1 custom-text">
                    <label class="col-form-label" for="xaa_aa_need_optimiser">Do You need product Optimisers ?</label>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <div>
                        <select name="xaa_aa_need_optimiser" class="form-control">
                            <option></option>
                            <option value='ja' t-att-selected="formulier_id.xaa_aa_need_optimiser == 'ja'">
                                ja
                            </option>
                            <option value='nee' t-att-selected="formulier_id.xaa_aa_need_optimiser == 'nee'">
                                nee
                            </option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="form-group row form-field optimiser_field" t-att-style="'display:block;' if formulier_id.xaa_aa_need_optimiser == 'ja' else 'display:none;'">
                <div class="div_padding_left" >
                    <div class="col-lg-3 col-md-4 col-sm-4 col-lg-offset-1 col-md-offset-1 custom-text">
                        <label class="col-form-label" for="xaa_aa_optimiser_product">Which optimizer do you want ?</label>
                    </div>
                    <div class="row col-lg-12 col-md-12 col-sm-12">
                        <div class="optimiser_product_section"
                            style="display:block; list-style-type:none;">
                            <t t-foreach="optimiser_products" t-as="op">
                                <div t-att-class="'col-lg-6 col-md-6 col-sm-6 col-12 padding_14_10 optimiser_product_li product_li selected text-center mt8' if formulier_id.xaa_aa_optimiser_product and formulier_id.xaa_aa_optimiser_product.id == op.id else 'col-lg-6 col-md-6 col-sm-6 col-12 padding_14_10 optimiser_product_li product_li text-center mt8'" t-att-data-id="op.id">
                                    <div class="text-center mt4">
                                        <span t-if="op.xaa_aa_wq_header_1" style="font-size:16px;" t-esc="op.xaa_aa_wq_header_1"/>
                                        <span t-if="not op.xaa_aa_wq_header_1" style="font-size:16px;" t-esc="op.name"/><br/>
                                        <span t-if="op.xaa_aa_wq_header_2" style="font-size:14px;" t-esc="op.xaa_aa_wq_header_2"/>
                                    </div>
                                    <div class="img_house_info">
                                        <img class="img img-fluid" t-attf-src="/web/image/product.product/{{op.id}}/xaa_aa_wq_image"/>
                                    </div>
                                    <div class="text-left">
                                        <t t-if="op.xaa_aa_wq_line1">
                                            <b><span style="font-size:12px;" t-esc="op.xaa_aa_wq_line1"/></b><br/>
                                        </t>
                                        <t t-if="op.xaa_aa_wq_line2">
                                            <b><span style="font-size:12px;" t-esc="op.xaa_aa_wq_line2"/></b><br/>
                                        </t>
                                        <t t-if="op.xaa_aa_wq_line3">
                                            <b><span style="font-size:12px;" t-esc="op.xaa_aa_wq_line3"/></b><br/>
                                        </t>
                                        <t t-if="op.xaa_aa_wq_line4">
                                            <b><span style="font-size:12px;" t-esc="op.xaa_aa_wq_line4"/></b>
                                        </t>
                                    </div>
                                </div>
                            </t>
                        </div>
                        <div class="optimiser_product_qty_show" t-att-style="'display:block;' if formulier_id.xaa_aa_optimiser_product else 'display:none;'">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="snipper_Add_Minus mt24">
                                    <button type="button" class="pf_sub remove_optimiser_qty update_optimiser_qty" t-att-value="formulier_id.xaa_aa_optimiser_product.id">-</button>
                                    <span class="product_qty o_qty optimiser_qty">1</span>
                                    <button type="button" class="pf_add add_optimiser_qty update_optimiser_qty" t-att-value="formulier_id.xaa_aa_optimiser_product.id">+</button>
                                    <span class="montserrat_regular ml8 mb8">QTY</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-group row form-field">
                <div class="col-lg-3 col-md-4 col-sm-4 col-lg-offset-1 col-md-offset-1 custom-text">
                    <label class="col-form-label" for="xaa_aa_pf_select_roof">Is it a flat roof or a slanted roof ?</label>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <div>
                        <select name="xaa_aa_pf_select_roof" class="form-control">
                            <option></option>
                            <option value='Flat Roof' t-att-selected="formulier_id.xaa_aa_pf_select_roof == 'Flat Roof'">
                                Flat Roof
                            </option>
                            <option value='Slanted Roof' t-att-selected="formulier_id.xaa_aa_pf_select_roof == 'Slanted Roof'">
                                Slanted Roof
                            </option>
                            <option value='Mix Roof' t-att-selected="formulier_id.xaa_aa_pf_select_roof == 'Mix Roof'">
                                Mix Roof
                            </option>

                        </select>
                    </div>
                </div>
            </div>
            <div class="form-group row form-field flat_product" t-att-style="'display:block;' if formulier_id.xaa_aa_pf_select_roof in ['Flat Roof','Mix Roof'] else 'display:none;'">
                <div class="div_padding_left" >
                    <div class="col-lg-3 col-md-4 col-sm-4 col-lg-offset-1 col-md-offset-1 custom-text">
                        <label class="col-form-label" for="formulier_id.xaa_aa_flat_roof_product">Which Flat roof system You want ?</label>
                    </div>
                    <div class="row col-lg-12 col-md-12 col-sm-12">
                        <div class="flat_product_section"
                            style="display:block; list-style-type:none;">
                            <t t-foreach="flat_product_ids" t-as="frp">
                                <div t-att-class="'col-lg-6 col-md-6 col-sm-6 col-12 padding_14_10 flat_product_li product_li selected text-center mt8' if formulier_id.xaa_aa_flat_roof_product and formulier_id.xaa_aa_flat_roof_product.id == frp.id else 'col-lg-6 col-md-6 col-sm-6 col-12 padding_14_10 flat_product_li product_li text-center mt8'" t-att-data-id="frp.id">
                                    <div class="text-center mt4">
                                        <span t-if="frp.xaa_aa_wq_header_1" style="font-size:16px;" t-esc="frp.xaa_aa_wq_header_1"/>
                                        <span t-if="not frp.xaa_aa_wq_header_1" style="font-size:16px;" t-esc="frp.name"/><br/>
                                        <span t-if="frp.xaa_aa_wq_header_2" style="font-size:14px;" t-esc="frp.xaa_aa_wq_header_2"/>
                                    </div>
                                    <div class="img_house_info">
                                        <img class="img img-fluid" t-attf-src="/web/image/product.product/{{frp.id}}/xaa_aa_wq_image"/>
                                    </div>
                                    <div class="text-left">
                                        <t t-if="frp.xaa_aa_wq_line1">
                                            <b><span style="font-size:12px;" t-esc="frp.xaa_aa_wq_line1"/></b><br/>
                                        </t>
                                        <t t-if="frp.xaa_aa_wq_line2">
                                            <b><span style="font-size:12px;" t-esc="frp.xaa_aa_wq_line2"/></b><br/>
                                        </t>
                                        <t t-if="frp.xaa_aa_wq_line3">
                                            <b><span style="font-size:12px;" t-esc="frp.xaa_aa_wq_line3"/></b><br/>
                                        </t>
                                        <t t-if="frp.xaa_aa_wq_line4">
                                            <b><span style="font-size:12px;" t-esc="frp.xaa_aa_wq_line4"/></b>
                                        </t>
                                    </div>
                                </div>
                            </t>
                        </div>
                        <div class="flat_product_qty_show" t-att-style="'display:block;' if formulier_id.xaa_aa_flat_roof_product else 'display:none;'">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="snipper_Add_Minus mt24">
                                    <button type="button" class="pf_sub remove_roof_qty update_roof_qty" t-att-value="formulier_id.xaa_aa_flat_roof_product.id">-</button>
                                    <span class="product_qty roof_qty flat_roof_qty">1</span>
                                    <button type="button" class="pf_add add_roof_qty update_roof_qty" t-att-value="formulier_id.xaa_aa_flat_roof_product.id">+</button>
                                    <span class="montserrat_regular ml8 mb8">QTY</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-group row form-field slanted_product" t-att-style="'display:block;' if formulier_id.xaa_aa_pf_select_roof in ['Slanted Roof','Mix Roof'] else 'display:none;'">
                <div class="div_padding_left" >
                    <div class="col-lg-3 col-md-4 col-sm-4 col-lg-offset-1 col-md-offset-1 custom-text">
                        <label class="col-form-label" for="formulier_id.xaa_aa_slanted_roof_product">Which Slanted roof system You want ?</label>
                    </div>
                    <div class="row col-lg-12 col-md-12 col-sm-12">
                        <div class="slanted_product_section"
                            style="display:block; list-style-type:none;">
                            <t t-foreach="slanted_product_ids" t-as="srp">
                                <div t-att-class="'col-lg-6 col-md-6 col-sm-6 col-12 padding_14_10 slanted_product_li product_li selected text-center mt8' if formulier_id.xaa_aa_slanted_roof_product and formulier_id.xaa_aa_slanted_roof_product.id == srp.id else 'col-lg-6 col-md-6 col-sm-6 col-12 padding_14_10 slanted_product_li product_li text-center mt8'" t-att-data-id="srp.id">
                                    <div class="text-center mt4">
                                        <span t-if="srp.xaa_aa_wq_header_1" style="font-size:16px;" t-esc="srp.xaa_aa_wq_header_1"/>
                                        <span t-if="not srp.xaa_aa_wq_header_1" style="font-size:16px;" t-esc="srp.name"/><br/>
                                        <span t-if="srp.xaa_aa_wq_header_2" style="font-size:14px;" t-esc="srp.xaa_aa_wq_header_2"/>
                                    </div>
                                    <div class="img_house_info">
                                        <img class="img img-fluid" t-attf-src="/web/image/product.product/{{srp.id}}/xaa_aa_wq_image"/>
                                    </div>
                                    <div class="text-left">
                                        <t t-if="srp.xaa_aa_wq_line1">
                                            <b><span style="font-size:12px;" t-esc="srp.xaa_aa_wq_line1"/></b><br/>
                                        </t>
                                        <t t-if="srp.xaa_aa_wq_line2">
                                            <b><span style="font-size:12px;" t-esc="srp.xaa_aa_wq_line2"/></b><br/>
                                        </t>
                                        <t t-if="srp.xaa_aa_wq_line3">
                                            <b><span style="font-size:12px;" t-esc="srp.xaa_aa_wq_line3"/></b><br/>
                                        </t>
                                        <t t-if="srp.xaa_aa_wq_line4">
                                            <b><span style="font-size:12px;" t-esc="srp.xaa_aa_wq_line4"/></b>
                                        </t>
                                    </div>
                                </div>
                            </t>
                        </div>
                        <div class="slanted_product_qty_show" t-att-style="'display:block;' if formulier_id.xaa_aa_slanted_roof_product else 'display:none;'">
